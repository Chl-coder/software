<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ff.sxbank.mapper.ApplyRecordMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.ff.sxbank.pojo.ApplyRecord">
        <id column="apply_id" property="applyId" />
        <result column="username" property="username" />
        <result column="identity_number" property="identityNumber" />
        <result column="apply_date" property="applyDate" />
        <result column="is_success" property="isSuccess" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        apply_id, username, identity_number, apply_date, is_success
    </sql>

</mapper>
